{
  "name": "storybook-addon-angular-router",
  "version": "1.3.0",
  "description": "Addon to enable logging and mocking for the angular router in storybook",
  "keywords": [
    "storybook-addons",
    "angular",
    "router"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jakob-em/storybook-addon-angular-router"
  },
  "author": "jakob-em",
  "license": "MIT",
  "main": "dist/preset",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "babel ./src --out-dir ./dist",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "start": "concurrently \"npm run storybook -- --no-manager-cache --quiet\" \"npm run build -- --watch\"",
    "build-storybook": "npm run build && build-storybook",
    "release": "npm run build && auto shipit"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.1.0",
    "@angular-devkit/core": "12.1.0",
    "@angular/cli": "12.1.0",
    "@angular/common": "12.1.0",
    "@angular/compiler": "12.1.0",
    "@angular/compiler-cli": "12.1.0",
    "@angular/core": "12.1.0",
    "@angular/forms": "12.1.0",
    "@angular/platform-browser": "12.1.0",
    "@angular/platform-browser-dynamic": "12.1.0",
    "@angular/router": "12.1.0",
    "@babel/cli": "7.14.5",
    "@babel/core": "7.14.6",
    "@babel/preset-env": "7.14.7",
    "@storybook/addon-actions": "6.3.0",
    "@storybook/addon-controls": "6.3.0",
    "@storybook/addon-docs": "6.3.0",
    "@storybook/angular": "6.3.0",
    "@storybook/builder-webpack5": "6.3.0",
    "@storybook/manager-webpack5": "6.3.0",
    "auto": "10.29.3",
    "babel-loader": "8.2.2",
    "concurrently": "6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "27.0.5",
    "rimraf": "3.0.2",
    "rxjs": "6.6.7",
    "terser-webpack-plugin": "^5.1.3",
    "zone.js": "0.11.4"
  },
  "peerDependencies": {
    "@angular/router": "^11.0.0 || ^12.0.0",
    "@storybook/addon-actions": "^6.3.0",
    "@storybook/addons": "^6.3.0",
    "@storybook/angular": "^6.3.0",
    "@storybook/api": "^6.3.0",
    "rxjs": "^6.6.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Angular Router",
    "supportedFrameworks": [
      "angular"
    ]
  }
}
