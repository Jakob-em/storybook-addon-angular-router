{
  "name": "storybook-addon-angular-router",
  "version": "1.3.1",
  "description": "Addon to enable logging and mocking for the angular router in storybook",
  "keywords": [
    "storybook-addons",
    "angular",
    "router"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jakob-em/storybook-addon-angular-router"
  },
  "author": "jakob-em",
  "license": "MIT",
  "main": "dist/preset",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "babel ./src --out-dir ./dist",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "start": "concurrently \"npm run storybook -- --no-manager-cache --quiet\" \"npm run build -- --watch\"",
    "build-storybook": "npm run build && build-storybook",
    "release": "npm run build && auto shipit"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.0.3",
    "@angular-devkit/core": "13.0.3",
    "@angular/cli": "13.0.3",
    "@angular/common": "13.0.2",
    "@angular/compiler": "13.0.2",
    "@angular/compiler-cli": "13.0.2",
    "@angular/core": "13.0.2",
    "@angular/forms": "13.0.2",
    "@angular/platform-browser": "13.0.2",
    "@angular/platform-browser-dynamic": "13.0.2",
    "@angular/router": "13.0.2",
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@storybook/addon-actions": "6.4.3",
    "@storybook/addon-controls": "6.4.3",
    "@storybook/addon-docs": "6.4.3",
    "@storybook/angular": "6.4.3",
    "@storybook/builder-webpack5": "6.4.3",
    "@storybook/manager-webpack5": "6.4.3",
    "@types/jest": "^27.0.3",
    "auto": "10.32.3",
    "babel-loader": "8.2.3",
    "concurrently": "6.4.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "27.3.1",
    "jest-preset-angular": "^11.0.0-rc.4",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0",
    "terser-webpack-plugin": "^5.2.5",
    "ts-jest": "^27.0.7",
    "typescript": "4.4.4",
    "zone.js": "0.11.4"
  },
  "peerDependencies": {
    "@angular/router": "^11.0.0 || ^12.0.0 || ^13.0.0",
    "@storybook/addon-actions": "^6.4.0",
    "@storybook/addons": "^6.4.0",
    "@storybook/angular": "^6.4.0",
    "@storybook/api": "^6.4.0",
    "rxjs": "^6.6.7 || ^7.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Angular Router",
    "supportedFrameworks": [
      "angular"
    ]
  }
}
