{
  "name": "storybook-addon-angular-router",
  "version": "0.0.1",
  "description": "Addon to enable logging and mocking for the angular router in storybook",
  "keywords": [
    "storybook-addons",
    "angular",
    "router"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jakob-em/storybook-addon-angular-router"
  },
  "author": "jakob-em",
  "license": "MIT",
  "main": "dist/preset",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "babel ./src --out-dir ./dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "start": "concurrently \"npm run storybook -- --no-manager-cache --quiet\" \"npm run build -- --watch\"",
    "build-storybook": "build-storybook",
    "release": "npm run build && auto shipit"
  },
  "devDependencies": {
    "@angular/router": "11.2.6",
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-controls": "^6.1.21",
    "@storybook/angular": "^6.1.21",
    "auto": "^10.3.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6"
  },
  "peerDependencies": {
    "@angular/router": "11.2.6",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addons": "^6.1.21",
    "@storybook/angular": "^6.1.21",
    "@storybook/api": "^6.1.21",
    "rxjs": "^6.6.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Angular Router",
    "supportedFrameworks": [
      "angular"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
