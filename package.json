{
  "name": "storybook-addon-angular-router",
  "version": "1.7.1",
  "description": "Addon to enable logging and mocking for the angular router in storybook",
  "keywords": [
    "storybook-addons",
    "angular",
    "router"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jakob-em/storybook-addon-angular-router"
  },
  "author": "jakob-em",
  "license": "MIT",
  "main": "dist/preset",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "babel ./src --out-dir ./dist",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "start": "concurrently \"npm run storybook -- --no-manager-cache --quiet\" \"npm run build -- --watch\"",
    "build-storybook": "npm run build && build-storybook",
    "release": "npm run build && auto shipit"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.4",
    "@angular-devkit/core": "15.0.4",
    "@angular/cli": "15.0.4",
    "@angular/common": "15.0.4",
    "@angular/compiler": "15.0.4",
    "@angular/compiler-cli": "15.0.4",
    "@angular/core": "15.0.4",
    "@angular/forms": "15.0.4",
    "@angular/platform-browser": "15.0.4",
    "@angular/platform-browser-dynamic": "15.0.4",
    "@angular/router": "15.0.4",
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.5",
    "@babel/plugin-proposal-decorators": "^7.20.5",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-controls": "6.5.14",
    "@storybook/addon-docs": "6.5.14",
    "@storybook/angular": "6.5.14",
    "@storybook/builder-webpack5": "6.5.14",
    "@storybook/manager-webpack5": "6.5.14",
    "@types/jest": "^28.1.8",
    "auto": "10.37.6",
    "babel-loader": "9.1.0",
    "concurrently": "7.6.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "28.1.3",
    "jest-preset-angular": "^12.2.3",
    "rimraf": "3.0.2",
    "rxjs": "7.6.0",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^28.0.8",
    "typescript": "4.8.4",
    "zone.js": "0.11.8"
  },
  "peerDependencies": {
    "@angular/router": "^11.0.0 || ^12.0.0 || ^13.0.0 || ^14.0.0 || ^15.0.0",
    "@storybook/addon-actions": "^6.4.0",
    "@storybook/addons": "^6.5.14",
    "@storybook/angular": "^6.5.14",
    "@storybook/api": "^6.5.14",
    "rxjs": "^6.6.7 || ^7.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Angular Router",
    "supportedFrameworks": [
      "angular"
    ]
  }
}
