{
  "name": "storybook-addon-angular-router",
  "version": "1.1.0",
  "description": "Addon to enable logging and mocking for the angular router in storybook",
  "keywords": [
    "storybook-addons",
    "angular",
    "router"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jakob-em/storybook-addon-angular-router"
  },
  "author": "jakob-em",
  "license": "MIT",
  "main": "dist/preset",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "babel ./src --out-dir ./dist",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "start": "concurrently \"npm run storybook -- --no-manager-cache --quiet\" \"npm run build -- --watch\"",
    "build-storybook": "npm run build && build-storybook",
    "release": "npm run build && auto shipit"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.6",
    "@angular-devkit/core": "^11.2.6",
    "@angular/cli": "^11.2.6",
    "@angular/common": "11.2.7",
    "@angular/compiler": "^11.2.7",
    "@angular/compiler-cli": "^11.2.7",
    "@angular/core": "11.2.7",
    "@angular/forms": "11.2.7",
    "@angular/platform-browser": "11.2.7",
    "@angular/platform-browser-dynamic": "11.2.7",
    "@angular/router": "11.2.7",
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@storybook/addon-actions": "^6.2.1",
    "@storybook/addon-controls": "^6.2.1",
    "@storybook/addon-docs": "^6.2.1",
    "@storybook/angular": "^6.2.1",
    "auto": "^10.24.1",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.0.0",
    "jest": "^26.6.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "zone.js": "0.11.4"
  },
  "peerDependencies": {
    "@angular/router": "^11.2.7",
    "@storybook/addon-actions": "^6.2.1",
    "@storybook/addons": "^6.2.1",
    "@storybook/angular": "^6.2.1",
    "@storybook/api": "^6.2.1",
    "rxjs": "^6.6.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Angular Router",
    "supportedFrameworks": [
      "angular"
    ]
  }
}
