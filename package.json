{
  "name": "storybook-addon-angular-router",
  "version": "1.8.0",
  "description": "Addon to enable logging and mocking for the angular router in storybook",
  "keywords": [
    "storybook-addons",
    "angular",
    "router"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jakob-em/storybook-addon-angular-router"
  },
  "author": "jakob-em",
  "license": "MIT",
  "main": "dist/preset",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "babel ./src --out-dir ./dist",
    "test": "jest",
    "storybook": "ng run angular-repro:storybook",
    "start": "concurrently \"npm run storybook\" \"npm run build -- --watch\"",
    "build-storybook": "npm run build && ng run angular-repro:build-storybook",
    "release": "npm run build && auto shipit"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.0.2",
    "@angular-devkit/core": "16.0.2",
    "@angular/cli": "16.0.2",
    "@angular/common": "16.0.1",
    "@angular/compiler": "16.0.1",
    "@angular/animations": "16.0.1",
    "@angular/compiler-cli": "16.0.1",
    "@angular/core": "16.0.1",
    "@angular/forms": "16.0.1",
    "@angular/platform-browser": "16.0.1",
    "@angular/platform-browser-dynamic": "16.0.1",
    "@angular/router": "16.0.1",
    "@babel/cli": "7.21.5",
    "@babel/core": "7.21.8",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/preset-env": "7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@storybook/addon-actions": "7.0.12",
    "@storybook/addon-controls": "7.0.12",
    "@storybook/addon-docs": "7.0.12",
    "@storybook/angular": "7.0.12",
    "@types/jest": "^29.5.1",
    "auto": "10.46.0",
    "babel-loader": "9.1.2",
    "concurrently": "8.0.1",
    "html-webpack-plugin": "^5.5.1",
    "jest": "29.5.0",
    "jest-preset-angular": "^13.1.1",
    "rimraf": "5.0.0",
    "rxjs": "7.8.1",
    "terser-webpack-plugin": "^5.3.8",
    "ts-jest": "^29.1.0",
    "typescript": "5.0.4",
    "zone.js": "0.13.0"
  },
  "peerDependencies": {
    "@angular/router": ">11.0.0 < 17.0.0",
    "@storybook/addon-actions": "^7.0.0",
    "@storybook/preview-api": "^7.0.0",
    "@storybook/angular": "^7.0.0",
    "@storybook/api": "^7.0.0",
    "rxjs": "^6.6.7 || ^7.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Angular Router",
    "supportedFrameworks": [
      "angular"
    ]
  }
}
